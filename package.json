{
  "name": "openscad-web-customizer-forge",
  "version": "1.10.0",
  "description": "Web-based OpenSCAD customizer with client-side WASM rendering",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "setup-wasm": "node scripts/download-wasm.js",
    "setup-libraries": "node scripts/setup-libraries.js",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "test": "echo 'Tests coming in Phase 3'",
    "check-a11y": "lighthouse http://localhost:5173 --only-categories=accessibility --quiet"
  },
  "keywords": [
    "openscad",
    "customizer",
    "3d-printing",
    "stl",
    "wasm"
  ],
  "author": "",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "jszip": "^3.10.1",
    "openscad-wasm-prebuilt": "^1.2.0",
    "three": "^0.160.0"
  }
}
